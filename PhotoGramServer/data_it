#quelle non composte sono ignorate
dello=del lo;dell'=del l';della=del la
dei=del i;degli=del gli;delle=del le
dalla=dal la;allo=al lo;all'=al l';alla=al la
ai=a i;agli=al gli;alle=al le;dallo=dal lo
dall'=dal l';dalla=dal la;dai=da i;dagli=da gli
dalle=dal le;nello=nel lo;nell'=nel l';nella=nel la
nei=nel i;negli=nel gli;nelle=nel le
sullo=sul lo;sull'=sul l';sulla=sul la
sui=su i;sugli=su gli;sulle=sul le